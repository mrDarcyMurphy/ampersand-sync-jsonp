  "description": "re-implement ampersand sync to do everything with jsonp",
  "main": "ampersand-sync-jsonp.js",
  "scripts": {
    "start": "run-browser test/*",
    "test": "browserify test/* | tape-run | tap-spec"
  },
  "keywords": [
    "ampersandjs",
    "jsonp"
  ],
  "author": "Darcy Murphy",
  "license": "MIT",
  "dependencies": {
    "qs": "^2.2.0",
    "underscore": "^1.7.0"
  },
